sole.log(string.length)\nlet n=-1\n\nlet string2=\"\"\nlet step=()=>{\n    n+=1\n    if(string[n]==='\\n'){\n        string2+=\"<br>\"\n    }else if(string[n]===' '){\n        string2+=\"&nbsp;\"\n    }else{\n        string2+=string[n]\n    }\n    setTimeout(()=>{   \n        console.log(n)\n        window.scrollTo(0,99999)\n        html.scrollTo(0,99999)\n        if(n<string.length){\n            html.innerHTML=string2\n            style.innerHTML=string.substring(0,n)\n            console.log(html.innerHTML)\n        }else{\n            return\n        }\n        step()\n    },50) \n}\nstep()\n// let style=document.querySelector(\"style\")\n\n// setTimeout(()=>{\n//     style.innerHTML=`\n//         body{\n//             color:red\n//         }\n//     `\n// },200)"},"lineCount":null}},"error":null,"hash":"a76882ffa2907d1508cd4af04973d39b","cacheData":{"env":{}}}